{
  "name": "readme-micro",
  "version": "2.0.0",
  "description": "A GitHub action for uploading OAS files to ReadMe Micro",
  "main": "index.js",
  "bin": {
    "@readme/micro": "index.js"
  },
  "scripts": {
    "pretest": "npm run lint && npm run prettier",
    "lint": "eslint .",
    "prettier": "prettier --check \"./**/**.{js,md,yaml,json}\"",
    "prettier:fix": "prettier --list-different --write \"./**/**.{js,md,yaml,json}\"",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.0.3",
    "axios": "^0.27.2",
    "ci-info": "^3.8.0",
    "command-line-args": "^5.2.1",
    "dotenv": "^16.0.1",
    "glob": "^8.0.3",
    "oas-normalize": "^8.4.0",
    "swagger-inline": "^6.1.0"
  },
  "devDependencies": {
    "@readme/eslint-config": "^10.5.2",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "nock": "^13.3.0",
    "prettier": "^2.8.7"
  },
  "prettier": "@readme/eslint-config/prettier"
}
